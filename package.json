{
    "name": "@mkrause/lifecycle-rest",
    "version": "0.1.2",
    "description": "Lifecycle REST API loader",
    "author": "mkrause",
    "license": "MIT",
    "repository": "https://github.com/mkrause/lifecycle-rest",
    "es2015": "src/index.js",
    "main": "lib-cjs/index.js",
    "module": "lib-esm/index.js",
    "browser": "lib-esm/index.js",
    "files": [
        "src/",
        "lib-cjs/",
        "lib-esm/",
        "typings/index.d.ts"
    ],
    "scripts": {
        "build:cjs": "NODE_ENV=production BABEL_ENV=cjs babel src --out-dir lib-cjs --extensions '.js,.ts' --source-maps inline",
        "build:esm": "NODE_ENV=production BABEL_ENV=esm babel src --out-dir lib-esm --extensions '.js,.ts' --source-maps inline",
        "types": "tsc --project ./tsconfig.decl.json",
        "test": "npm run build:esm && NODE_ENV=development BABEL_ENV=cjs mocha --require=@babel/register --recursive tests",
        "prepublishOnly": "npm run build:esm && npm run build:cjs && npm run types"
    },
    "types": "./typings/index.d.ts",
    "devDependencies": {
        "@babel/core": "^7.6.4",
        "@babel/register": "^7.6.2",
        "@babel/cli": "^7.6.4",
        "@babel/preset-env": "^7.6.3",
        "@babel/plugin-proposal-class-properties": "^7.5.5",
        "babel-plugin-transform-builtin-extend": "^1.1.2",
        "@babel/preset-typescript": "^7.6.0",
        "typescript": "^3.6.4",
        "@types/node": "^12.12.4",
        "tsd": "^0.10.0",
        "mocha": "^6.2.2",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "chai-match-pattern": "^1.1.0",
        "sinon": "^7.5.0",
        "redux": "^4.0.4"
    },
    "peerDependencies": {
        "redux": "^4.0.4"
    },
    "dependencies": {
        "uuid": "^3.3.3",
        "case-match": "^2.0.0",
        "moment": "^2.24.0",
        "immutable": "^4.0.0-rc.12",
        "uri-tag": "^1.1.3",
        "message-tag": "^0.4.2",
        "object-merge-advanced": "^10.11.4",
        "http-status-codes": "^1.4.0",
        "axios": "^0.19.0",
        "@mkrause/lifecycle-loader": "^0.0.4",
        "ts-toolbelt": "^4.9.32",
        "fp-ts": "^2.1.1",
        "io-ts": "^2.0.1"
    }
}
