{
    "name": "@mkrause/lifecycle-rest",
    "version": "0.0.0",
    "description": "Lifecycle REST API loader",
    "author": "mkrause",
    "license": "MIT",
    "repository": "https://github.com/mkrause/lifecycle-rest",
    "es2015": "src/index.js",
    "main": "lib-cjs/index.js",
    "module": "lib-esm/index.js",
    "browser": "lib-esm/index.js",
    "files": [
        "src/",
        "lib-cjs/",
        "lib-esm/"
    ],
    "scripts": {
        "build:cjs": "NODE_ENV=production BABEL_ENV=cjs babel src --out-dir lib-cjs --source-maps inline",
        "build:esm": "NODE_ENV=production BABEL_ENV=esm babel src --out-dir lib-esm --source-maps inline",
        "test": "NODE_ENV=development BABEL_ENV=cjs mocha --require babel-core/register --recursive tests",
        "prepublishOnly": "npm run build:esm && npm run build:cjs && flow-copy-source src lib-cjs"
    },
    "devDependencies": {
        "babel-core": "^6.26.0",
        "babel-cli": "^6.26.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-plugin-transform-builtin-extend": "^1.1.2",
        "babel-preset-flow": "^6.23.0",
        "babel-preset-env": "^1.6.1",
        "flow-bin": "^0.70.0",
        "flow-copy-source": "^1.3.0",
        "mocha": "^5.1.1",
        "chai": "^4.1.2"
    },
    "dependencies": {
        "uuid": "^3.2.1",
        "lodash": "^4.17.5",
        "immutable": "^4.0.0-rc.9",
        "uri-tag": "^0.0.1",
        "message-tag": "^0.2.1",
        "object-merge-advanced": "^10.1.0",
        "axios": "^0.18.0"
    }
}
