{
    "name": "@mkrause/lifecycle-rest",
    "version": "0.5.4",
    "author": "mkrause",
    "license": "MIT",
    "repository": "https://github.com/mkrause/lifecycle-rest",
    "description": "Lifecycle REST API loader",
    "keywords": [],
    "files": [
        "src/",
        "lib-cjs/",
        "lib-esm/",
        "typings/"
    ],
    "typings": "./typings/index.d.ts",
    "engines": {
        "node": ">= 12.13"
    },
    "main": "./lib-cjs/index.js",
    "module": "./lib-esm/index.js",
    "scripts": {
        "_build": "babel src --extensions '.js,.ts' --source-maps=true",
        "build:cjs": "NODE_ENV=production BABEL_ENV=cjs npm run _build -- --out-dir lib-cjs --delete-dir-on-start",
        "build:esm": "NODE_ENV=production BABEL_ENV=esm npm run _build -- --out-dir lib-esm --delete-dir-on-start",
        "build:types": "tsc --project ./tsconfig.decl.json",
        "test:unit": "NODE_ENV=development mocha --require=ts-node/register --recursive tests/unit",
        "test:integration": "NODE_ENV=development mocha --require=ts-node/register --recursive tests/integration",
        "test": "npm run test:unit && npm run test:integration && tsc && echo '[tsc] success'",
        "prepublishOnly": "npm run build:esm && npm run build:cjs && npm run build:types"
    },
    "devDependencies": {
        "@babel/core": "^7.15.0",
        "@babel/register": "^7.14.5",
        "@babel/cli": "^7.14.8",
        "@babel/preset-env": "^7.15.0",
        "@babel/preset-typescript": "^7.15.0",
        "babel-plugin-transform-builtin-extend": "^1.1.2",
        "typescript": "^4.3.5",
        "ts-node": "^10.1.0",
        "tsd": "^0.17.0",
        "@types/node": "^16.4.13",
        "@types/uuid": "^8.3.1",
        "http-status-codes": "^2.1.4",
        "immutable": "^4.0.0-rc.14",
        "redux": "^4.1.1",
        "mocha": "^9.0.3",
        "@types/mocha": "^9.0.0",
        "chai": "^4.3.4",
        "@types/chai": "^4.2.21",
        "chai-as-promised": "^7.1.1",
        "@types/chai-as-promised": "^7.1.4",
        "chai-match-pattern": "^1.2.0",
        "sinon": "^11.1.2",
        "@types/sinon": "^10.0.2"
    },
    "peerDependencies": {
        "redux": "^4.0.0"
    },
    "dependencies": {
        "uuid": "^8.3.2",
        "uri-tag": "^1.4.0",
        "message-tag": "^0.6.0",
        "case-match": "^2.2.0",
        "object-merge-advanced": "^12.1.0",
        "axios": "^0.21.0",
        "fp-ts": "^2.11.1",
        "io-ts": "^2.2.16",
        "ts-toolbelt": "9.6.0",
        "@mkrause/lifecycle-loader": "^0.3.2"
    }
}
